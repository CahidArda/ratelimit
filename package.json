{
  "version": "0.1.0",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "description": "An HTTP/REST based Redis client built on top of Upstash REST API.",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest -i",
    "fmt": "rome format .",
    "lint": "eslint --ext .ts --fix --ignore-path .gitignore .",
    "build": "tsup && cp package.json ./dist/ && pnpm size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upstash/ratelimit.git"
  },
  "keywords": [
    "redis",
    "database",
    "serverless",
    "edge",
    "upstash"
  ],
  "author": "Andreas Thomas <andreas.thomas@chronark.com>",
  "bugs": {
    "url": "https://github.com/upstash/ratelimit/issues"
  },
  "homepage": "https://github.com/upstash/ratelimit#readme",
  "directories": {
    "examples": "examples"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "5 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "5 KB"
    }
  ],
  "peerDependencies": {
    "@upstash/redis": "^1.3.0-alpha.0"
  },
  "devDependencies": {
    "@jest/globals": "^27.4.6",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@upstash/redis": "^1.3.0-alpha.0",
    "dotenv": "^12.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rome": "^0.4.2-next",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.3",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  }
}
